    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "attach",
        "name": "Attach to process",
        "processId": "${command:PickProcess}",
        "skipFiles": ["<node_internals>/**"],
        "restart": true
      }
    ]
  }


